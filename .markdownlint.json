{
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD013": {
    "line_length": 120,
    "code_blocks": false,
    "tables": false
  },
  "MD022": {
    "lines_above": 1,
    "lines_below": 1
  },
  "MD024": {
    "allow_different_nesting": true
  },
  "MD025": {
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD033": {
    "allowed_elements": [
      "details",
      "summary",
      "br",
      "kbd",
      "mark",
      "sub",
      "sup"
    ]
  },
  "MD036": false,
  "MD041": false,
  "MD046": {
    "style": "fenced"
  },
  "MD049": {
    "style": "underscore"
  },
  "MD050": {
    "style": "asterisk"
  }
}
